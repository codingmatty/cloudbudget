{
  "name": "cloudbudget",
  "version": "0.1.0",
  "description": "CloudBudget",
  "main": "index.js",
  "scripts": {
    "watch": "./node_modules/.bin/babel src -d dist --source-maps -w",
    "dev": "concurrent 'npm run watch' 'nodemon -d 1 -w dist ./dist/app'",
    "build": "./node_modules/.bin/babel src -d dist",
    "prestart": "npm run build",
    "start": "NODE_ENV=production POST=8279 forever start -a -l cloudbudget ./dist/app.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test mocha --compilers js:babel-register",
    "preversion": "npm install && npm test && npm run lint",
    "version": "npm run build && git add -Af dist",
    "postversion": "git push && git push --tags && git rm -r dist && git commit -m 'Clean Build v%s'"
  },
  "repository": {
    "type": "git",
    "url": "git://bitbucket.org/mattjdev/cloudbudget.git"
  },
  "author": "Matthew Jacobs <hello@mattjdev.com>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/mattjdev/cloudbudget#readme",
  "dependencies": {
    "async": "^1.5.0",
    "bcrypt": "^0.8.5",
    "body-parser": "^1.14.2",
    "compression": "^1.5.2",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "inflection": "^1.8.0",
    "is_js": "^0.7.4",
    "jsonwebtoken": "^5.5.0",
    "lodash": "^3.10.1",
    "moment": "^2.11.0",
    "mongo-store": "0.0.2",
    "mongoose": "^4.3.4",
    "morgan": "^1.6.1",
    "oauth2orize": "^1.2.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^1.2.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "random-key": "^0.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "chai-js-factories": "^0.1.5",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "mocha": "^2.3.4",
    "source-map-support": "^0.4.0",
    "supertest": "^1.1.0",
    "timekeeper": "0.0.5"
  }
}
